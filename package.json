{
  "moduels": "node16",
  "name": "electron-wallet",
  "version": "0.0.1",
  "description": "electron based crypto wallet",
  "mainOld": "./dist/main.js",
  "scripts": {
    "buildWalletReactApp": "rm -rf static/apps/reactWallet && parcel build --dist-dir static/apps/reactWallet/ apps/reactWallet/src/index.html && sed -i 's/ src=\"\\\// src=\".\\\//g' static/apps/reactWallet/index.html",
    "buildExampleReactApp": "rm -rf static/apps/reactApp && parcel build --dist-dir static/apps/reactApp apps/reactApp/index.html && npm run reactAppIndexAppFix",
    "reactAppIndexAppFix": "sed -i 's/\\\/index/.\\\/index/' static/apps/reactApp/index.html",
    "buildRunTmp": "npx tsc src/testing.ts && sed -i 's/bip39_light_1\\.default/bip39_light_1/g' src/testing.js && node src/testing.js",
    "serve": "webpack-dev-server",
    "efstart": "electron-forge start",
    "efpackage": "electron-forge package",
    "efmake": "electron-forge make",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run compile && electron ./dist/main.js",
    "devReact": "parcel src/appReact.html",
    "devTake2": "npm run compile && electron ./dist/electron-app.js",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc && cp src/*renderer.js dist/ && sed -i 's/\\.default//g' dist/main.js",
    "start": "electron ./dist/main.js",
    "startjs": "electron ./js/src/main.js",
    "watch": "tsc -w",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "author": "Gavin Francis <gav@eh7.co.uk>",
  "license": "ISC",
  "dependencies": {
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "axios": "^1.4.0",
    "bip39-light": "^1.0.7",
    "bootstrap": "^5.2.3",
    "electron-squirrel-startup": "^1.0.0",
    "ethereumjs-util": "^7.1.5",
    "ethereumjs-wallet": "^1.0.2",
    "ethers": "^5.5.3",
    "fs": "^0.0.1-security",
    "hello-wasm": "^6.6.6",
    "parcel": "^2.8.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1",
    "react-table": "^7.8.0",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "@types/electron": "^1.6.10",
    "@types/node": "^14.11.2",
    "electron": "^24.2.0",
    "gts": "^3.1.1",
    "process": "^0.11.10",
    "typescript": "^5.0.4"
  }
}
