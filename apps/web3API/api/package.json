{
  "name": "api",
  "version": "0.0.1",
  "description": "REST api for the properties database",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node --unhandled-rejections=strict ./dist/index.js",
    "startdb": "tsc && node --unhandled-rejections=strict ./dist/mongodb.js",
    "debug": "export DEBUG=* && npm run start",
    "test": "jest --detectOpenHandles --forceExit",
    "testOld": "echo \"Error: no test specified\" && exit 1",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "author": "Gavin Francis <gav@eh7.co.uk>",
  "license": "ISC",
  "dependencies": {
    "@types/mongoose": "^5.11.97",
    "axios": "^1.3.5",
    "bson": "^5.4.0",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "iconv-lite": "^0.6.3",
    "jsonwebtoken": "^9.0.1",
    "mongodb-memory-server": "^8.13.0",
    "mongoose": "^7.0.3",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^14.11.2",
    "@types/supertest": "^2.0.12",
    "gts": "^3.1.1",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.3"
  }
}
